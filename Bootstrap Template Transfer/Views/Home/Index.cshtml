                                            <div class="col-sm-6">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <!-- Inputs -->
                                                        <input id="fizzInput" class="form-control" type="number" placeholder="Fizz" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <input id="buzzInput" class="form-control" type="number" placeholder="Buzz" />
                                                    </div>
                                                </div>
                                                <!-- Buttons -->
                                                <div class="row">
                                                    <div class="col-sm-12">
                                                        <!--Run Button-->
                                                        <button class="btn btn-lg btn-primary" id="btnFBRun">Submit</button>
                                                        <!--Code Button-->
                                                        <button id="btnToggleFB" class="btn btn-lg btn-success">Show Code</button>
                                                        <!--Clear Button-->
                                                        <button class="btn btn-lg btn-danger" id="btnFBClear">Clear</button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <p id="fbOutput">
                                                    <!-- Output Here-->

                                                </p>
                                            </div>
                                        </div>
                                        <!-- Code display-->
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div id="code-display-fb">
                                                    <pre class="brush: js">
                                                        $("#btnFBRun").click(function () {
                                                        //Store Inputs
                                                        var fizzNum = +$("#fizzInput").val();
                                                        var buzzNum = +$("#buzzInput").val();
                                                        var fbNum = fizzNum * buzzNum;
                                                        //Form array
                                                        var outputArray = []
                                                        //fill array 1-100
                                                        for (var i = 1; i < 101; i++) {
                                                            if (i % fbNum == 0) {
                                                            //isolate numbers divided by both
                                                            outputArray.push("<span id='fizzbuzzcolor'>FizzBuzz</span>");
                                                        } else if (i % fizzNum == 0) {
                                                            //isolate numbers divided by fizz
                                                            outputArray.push("<span id='fizzcolor'>Fizz</span>");
                                                        } else if (i % buzzNum == 0) {
                                                            //isolate numbers divided by buzz
                                                            outputArray.push("<span id='buzzcolor'>Buzz</span>");
                                                        } else outputArray.push(i);
                                                        }
                                                        //convert array into string
                                                        var fbArray = outputArray.join(' ');
                                                        //display results
                                                        $("#fbOutput").html(fbArray);
                                            
                                                    </pre>
                                                </div>
                                            </div>
