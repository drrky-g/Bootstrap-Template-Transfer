                                        <h3>Factorial</h3>
                                        <p>Find the factorial of a number.</p>
                                        <!-- Factorial UI -->

                                        <div class="row">
                                            <div class="col-sm-6">
                                                <!-- Input Box-->
                                                <input id="factInput" class="form-control" type="number" placeholder="Enter a number" />
                                            </div>
                                            <div class="col-sm-6">
                                                <p id="factOutput">
                                                    <!--Factorial Output Result Shown Here-->

                                                </p>
                                            </div>
                                        </div>



                                        <div class="row">
                                            <div class="col-sm-12">
                                                <!--Calculate Button-->
                                                <button class="btn btn-lg btn-primary" id="startFactor">Calculate</button>
                                                <!--Code Button-->
                                                <button id="btnToggleFact" class=" btn btn-lg btn-success">Show Code</button>
                                                <!--Clear Button-->
                                                <button class="btn btn-lg btn-danger" id="clearFactor">Clear</button>
                                            </div>
                                        </div>
                                        <!-- Code display-->
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div id="code-display-fact">
                                                    <pre class="brush: js">
                                            <!--put code here-->
                                            //Listener for Button Press
                                                $("#startFactor").click(function () {
                                                    //Collect Data
                                                    var factEntry = +$("#factInput").val();
                                                    //Manipulate Data
                                                    var factoredOutput = Factorial(factEntry);
                                                    //Output Manipulated Data         
                                                    $("#factOutput").text(factEntry + " has a factorial of " + factoredOutput + ".")
                                                });
                
                                            //Factorial function being called by factoredOutput variable
                                                function Factorial(num) {
                                                    if (num > 0) {
                                                        for (var loop = num - 1; loop > 1; loop--) {
                                                            num *= loop;
                                                        }
                                                    }
                                                    else if (num == 0) {
                                                        num = 1;
                                                    }
                                                    else {
                                                        num = "undefined";
                                                    }
                                                    return num;

                                                }
                                        </pre>
