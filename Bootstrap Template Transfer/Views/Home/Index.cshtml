                                        <h3>Basic Math</h3>
                                        <p>
                                            Find the lowest and highest values of a set of numbers.
                                            Also get the sum, product, and mean of the set of numbers.
                                        </p>
                                        <!-- Basic Math UI -->
                                        <!--Input Row-->
                                        <div class="row">
                                            <div class="col-sm-2">
                                                <input type="number" id="num1" class="form-control" placeholder="1" />
                                            </div>
                                            <div class="col-sm-2">
                                                <input type="number" id="num2" class="form-control" placeholder="2" />
                                            </div>
                                            <div class="col-sm-2">
                                                <input type="number" id="num3" class="form-control" placeholder="3" />
                                            </div>
                                            <div class="col-sm-2">
                                                <input type="number" id="num4" class="form-control" placeholder="4" />
                                            </div>
                                            <div class="col-sm-2">
                                                <input type="number" id="num5" class="form-control" placeholder="5" />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-sm-12">
                                                <!--Run Button-->
                                                <button class="btn btn-lg btn-primary" id="mathbtn">Submit</button>
                                                <!--Code Button-->
                                                <button id="btnToggleMath" class="btn btn-lg btn-success">Show Code</button>
                                                <!--Clear Button-->
                                                <button class="btn btn-lg btn-danger" id="mathclear">Clear</button>
                                            </div>
                                        </div>
                                        <!-- Output Rows-->

                                        <div class="row">
                                            <div class="col-sm-12 text-center">
                                                <p id="smallest">
                                                    <!-- Lowest Number Here-->
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 text-center">
                                                <p id="largest">
                                                    <!-- Lowest Number Here-->
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 text-center">
                                                <p id="mean">
                                                    <!-- Lowest Number Here-->
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 text-center">
                                                <p id="product">
                                                    <!-- Lowest Number Here-->
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 text-center">
                                                <p id="sum">
                                                    <!-- Lowest Number Here-->
                                                </p>
                                            </div>
                                        </div>
                                        <!-- Code display-->
